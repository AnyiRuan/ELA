{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/details/:path*",
      "destination": "/details/:path*"
    }
  ],
  "headers": [
    {
      "source": "/details/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "ANTHROPIC_API_KEY": "@anthropic_api_key"
  }
}
